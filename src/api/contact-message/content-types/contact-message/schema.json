{
  "kind": "collectionType",
  "collectionName": "contact_messages",
  "info": {
    "singularName": "contact-message",
    "pluralName": "contact-messages",
    "displayName": "contact message"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "richtext",
      "required": true,
      "maxLength": 100
    },
    "contactStatus": {
      "type": "enumeration",
      "default": "new",
      "enum": [
        "new",
        "read"
      ]
    }
  }
}
